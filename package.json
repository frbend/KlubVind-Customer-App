{
  "name": "klubvind-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "concurrently": "^7.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.3",
    "nodemon": "^2.0.19",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-window": "^1.8.7",
    "reactstrap": "^9.1.1",
    "serve": "^14.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "install-client": "cd client && npm install",
    "heroku-build": "concurrently --kill-others-on-fail \"npm run server\" \"npm run build --prefix client\" \"npm run serve-server\"",
    "netlify-build": "concurrently \"npm run server\" \"npm run build\"",
    "pre-build": "cd server && node app.js",
    "post-build": "cd client && serve -s build",
    "server": "cd server && node app.js",
    "build": "cd client && npm run build",
    "dev-server": "cd server && nodemon app.js",
    "serve-server": "cd client && serve -s build",
    "start": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run start --prefix client\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "overrides": {
    "autoprefixer": "10.4.5"
  }
}
